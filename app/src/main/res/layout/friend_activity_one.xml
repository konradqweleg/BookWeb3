<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    android:layout_width="match_parent"
    android:layout_marginLeft="20dp"
    android:layout_marginRight="20dp"
    android:orientation="vertical"
    android:padding="20dp"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="wrap_content">
<LinearLayout
    android:background="@color/colorAccent2"
 android:layout_width="250dp"
    android:orientation="vertical"
    android:layout_height="wrap_content">




    <TextView
        android:layout_width="match_parent"
        android:textAlignment="center"
        android:text="Ocena"
        android:textSize="25sp"
        android:textColor="#fff"
        android:id="@+id/FriendOne_nameField"
        android:layout_height="40dp"></TextView>
    <ImageView
        android:layout_width="150dp"
        android:layout_below="@+id/FriendOne_nameField"
        android:background="@drawable/blazej"
      android:layout_marginTop="10dp"
        android:layout_marginLeft="30dp"
        android:id="@+id/FriendOne_imageFriend"
        android:layout_marginRight="30dp"
        android:layout_gravity="center"
        android:layout_height="150dp"></ImageView>
    <TextView
        android:layout_width="match_parent"
        android:textAlignment="center"
        android:layout_below="@+id/FriendOne_imageFriend"
        android:layout_marginTop="10dp"
        android:id="@+id/FriendOne_markFriend"
        android:text="8"
        android:textSize="28sp"
        android:layout_height="40dp"></TextView>
    <TextView
        android:layout_width="match_parent"
        android:textAlignment="center"
        android:layout_below="@+id/FriendOne_markFriend"
        android:layout_marginTop="10dp"
        android:id="@+id/FriendOne_nameFriend"
        android:text="Błażej Jasiński"
        android:textSize="28sp"
        android:layout_height="40dp"></TextView>




</LinearLayout>
</FrameLayout>